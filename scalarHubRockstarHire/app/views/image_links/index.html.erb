<br>
  <div class="row">
    <% @image_links.each do |image_link| %>
      <div class="col-md-4">
        <% if image_link.url.present? %>
          <%= link_to image_tag(image_link.url, alt: "Image", style: "width: 430px; height: 300px;"), image_link %></br>
        <% else %>
          <p>No image available.</p>
        <% end %></br>
      </div>
    <% end %>
  </div>

