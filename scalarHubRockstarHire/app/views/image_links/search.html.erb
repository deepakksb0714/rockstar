<h2>Search Results for Tag: <%= @tag %></h2>
<div id="image_links">
<% if @search_results.present? %>
  
    <% @search_results.each do |image_link| %>

   
   <%= link_to image_tag(image_link.url, alt: "Image", style: "width: 430px; height: 300px;"), image_link %>
           
   <% end %>
  
<% else %>
  <p>No results found for tag: <%= @tag %></p>
<% end %>
</div>
